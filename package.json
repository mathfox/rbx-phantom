{
	"name": "@rbxts/phantom",
	"description": "Data manipulation library for Roblox.",
	"version": "0.1.0",
	"scripts": {
		"serve": "rojo serve default.project.json",
		"serve:tests": "rojo serve tests.project.json",
		"serve:bench": "rojo serve benchmarks.project.json",
		"check": "bunx @biomejs/biome check --write",
		"build:bench": "rojo sourcemap benchmarks.project.json --output sourcemap.json"
	},
	"main": "src/init.luau",
	"typings": "src/index.d.ts",
	"files": ["src", "!**/*.spec.luau", "!**/*.bench.luau"],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mathfox/rbx-phantom.git"
	},
	"keywords": ["roblox", "roblox-ts", "table", "array", "dictionary", "record", "map", "set", "immutable"],
	"author": {
		"name": "Ivan Leontev",
		"url": "https://github.com/mathfox"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mathfox/rbx-phantom/issues"
	},
	"homepage": "https://github.com/mathfox/rbx-phantom#readme",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@rbxts/compiler-types": "3.0.0-types.0",
		"@rbxts/types": "1.0.805",
		"@rbxts/jest": "^0.1.0",
		"@rbxts/jest-globals": "^0.1.0",
		"roblox-ts": "^3.0.0-dev-d1d5486",
		"typescript": "^5.7.0-dev.20240913"
	}
